{
  "version": 8,
  "name": "MapLibre Demo",
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "sources": {
    "osm": {
      "type": "vector",
      "tiles": [
        "https://demotiles.maplibre.org/tiles/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#EEEEEE"
      }
    },
    {
      "id": "osm-buildings",
      "type": "fill",
      "source": "osm",
      "source-layer": "building",
      "paint": {
        "fill-color": "#888888",
        "fill-opacity": 0.5
      }
    }
  ]
}
